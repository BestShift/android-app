<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bestshift_as">

    <application>
    <activity android:name=".StartframeActivity">
        android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.main"/>
        </intent-filter>
    </activity>
    <activity android:name=".MyActivity"
        android:screenOrientation="portrait"
        android:label="@string/app_name">
        <intent-filter>
            <action android:name="android.intent.action.VIEW"/>
            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>
    <activity android:name=".Kommfort.KammsherKreis"/>
    <activity android:name=".Kommfort.Beschleunigung"/>
    <activity android:name=".Stop.Stop"
        android:screenOrientation="portrait"/>
    <activity android:name=".EndeFahrt.Endanzeige"
        android:screenOrientation="portrait"/>

    </application>

</manifest>
